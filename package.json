{
  "name": "xstream-boilerplate",
  "version": "0.0.0",
  "description": "Sparks.network frontend",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TylorS/xstream-boilerplate.git"
  },
  "keywords": [
    "Cycle.js",
    "XStream"
  ],
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TylorS/xstream-boilerplate/issues"
  },
  "homepage": "https://github.com/TylorS/xstream-boilerplatey#readme",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "changelog": "conventional-changelog --infile CHANGELOG.md --same-file --release-count 0 --preset angular",
    "lint": "eslint src/*.js",
    "unit-test": "cross-env NODE_PATH=./src mocha -r babel-register",
    "e2e-test": "nightwatch",
    "coverage": "cross-env NODE_PATH=./src babel-node ./node_modules/.bin/isparta cover _mocha test/index.js",
    "test": "npm run lint && npm run unit-test && npm run e2e-test",
    "serve-static": "pushstate-server dist/ 8080 /200.html",
    "serve": "webpack-dev-server",
    "build": "webpack"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "node ./node_modules/validate-commit-msg/index.js"
    }
  },
  "dependencies": {
    "@cycle/dom": "^10.0.0-rc21",
    "@cycle/isolate": "^1.2.1",
    "@cycle/xstream-run": "^1.1.0",
    "cyclic-router": "^2.0.0",
    "normalize-css": "^2.3.1",
    "ramda": "^0.21.0",
    "xstream": "^2.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-espower": "^2.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-register-cli": "^2.0.1",
    "chromedriver": "^2.21.2",
    "commitizen": "^2.8.1",
    "conventional-changelog": "^1.1.0",
    "conventional-changelog-cli": "^1.2.0",
    "cross-env": "^1.0.7",
    "eslint": "^2.10.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "ghooks": "^1.2.1",
    "image-webpack-loader": "^1.8.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "mocha-circleci-reporter": "0.0.1",
    "nightwatch": "^0.8.18",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "postcss-loader": "^0.9.1",
    "power-assert": "^1.4.1",
    "pushstate-server": "^1.8.1",
    "selenium-server": "^2.53.0",
    "style-loader": "^0.13.1",
    "surge": "^0.18.0",
    "validate-commit-msg": "^2.6.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
